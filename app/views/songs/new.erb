<h1>Create a new song</h1>

<form class="" action="/songs" method="post">
  <label> Artist Name </label>
  <input type="text" name="artist[name]" id="Artist Name"> </br>

  <label> Song Name </label>
  <input type="text" name="song[name]" id="Name"> </br>

  <label>Genres:</label>
  <% @genres.each do |genre| %>
      <input type="checkbox" name="genres[]" id="<%= genre.name %>" value="<%= genre.id %>"><%=genre.name %>
  <% end %>
  </br></br>
  <input type="submit" value="Create">
</form>